{
  "name": "hotspot",
  "version": "1.0.0",
  "description": "<br> ##Introduction## ###Situation### > When a person wants to find a location to do X, they typically use multiple apps (ie. Yelp) and google searches to find what they need.",
  "main": "index.js",
  "engines": {
    "node": "6.2.2"
  },
  "scripts": {
    "dev:test": "babel-node server/server.js & mocha --compilers js:babel-register --bail --reporter nyan test/apiSpec.js; pkill -n node;",
    "dev:resetdb": "psql -f server/db/setuphotspotdb.sql",
    "dev:startdb": "postgres -D /usr/local/var/postgres",
    "dev:start": "npm run dev:buildclient; nodemon server/server.js --exec babel-node --presets es2015 stage-1 & npm run dev:startdb",
    "dev:buildclient": "webpack",
    "dev:config": "mkdir ./server/config; cp ../hotspot-config/config/* ./server/config/",
    "prod:buildserver": "babel ./server -d ./server-dist --presets es2015,stage-2 --copy-files",
    "start": "node server-dist/server.js",
    "deploy": "npm run buildserver; git add -A; git commit -m process.env.npm_package_config_m; git push origin master"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tremendous-Thumb/Hotspot.git"
  },
  "author": "Tremendous-Thumb",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Tremendous-Thumb/Hotspot/issues"
  },
  "homepage": "https://github.com/Tremendous-Thumb/Hotspot#readme",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.6",
    "bootstrap-validator": "^0.11.5",
    "eslint-config-hackreactor": "github:hackreactor-labs/eslint-config-hackreactor",
    "express": "^4.14.0",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.14.0",
    "font-awesome": "^4.6.3",
    "hbs": "^4.0.0",
    "jquery": "^2.2.4",
    "jquery-easing": "0.0.1",
    "lodash": "^4.6.1",
    "magnific-popup": "^1.1.0",
    "nonce": "^1.0.4",
    "oauth-signature": "^1.3.1",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-jwt": "^2.1.0",
    "passport-local": "^1.0.0",
    "pg": "^6.0.2",
    "pg-promise": "^5.1.3",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-materialize": "^0.14.4",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-devtools": "^3.3.1",
    "request": "^2.73.0",
    "scrollreveal": "^3.2.0",
    "tether": "^1.3.3",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.5.0",
    "chai": "^3.5.0",
    "mocha": "^2.5.3",
    "redux-devtools": "^3.3.1",
    "request": "^2.73.0"
  }
}
